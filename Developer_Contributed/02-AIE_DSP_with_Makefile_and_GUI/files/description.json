{
    "name": "Developer_Contributed-02-AIE_DSP_with_Makefile_and_GUI--make-flow",
    "description": "02-AIE_DSP_with_Makefile_and_GUI--make-flow",
    "gui": "false",
    "flow": "versal",

    "platform_allowlist": [ "vck190"],
    "platform_blocklist": [],

    "testinfo": {
        "test_name": "CTB_02-AIE_DSP",
        "jobs": [
            {
                "env": "",
                "cmd": "",
                "max_memory_MB": 65536,
                "max_time_min": {
                    "vitis_aie_sim": 50
                }
            }
        ],

        "targets": [
            "vitis_hw_build"
        ],
	"make_options": [
	    "VITIS_PLATFORM_XPFM='${PLATFORM_REPO_PATHS}/xilinx_vck190_base_202310_1/xilinx_vck190_base_202310_1.xpfm'",
	    "VITIS_SYSROOTS='${COMMON_IMAGE_VERSAL}/sysroots/cortexa72-cortexa53-xilinx-linux'",
	    "SDKTARGETSYSROOT='${COMMON_IMAGE_VERSAL}/sysroots/cortexa72-cortexa53-xilinx-linux'",
	    "KERNEL_IMAGE='${COMMON_IMAGE_VERSAL}/Image'",
	    "ROOTFS='${COMMON_IMAGE_VERSAL}/rootfs.ext4'"
	    ],
        "custom_build_target": {
            "all": "all",
            "hw": "all"
        },


        "tasks": {
           "build": {
               "pre_exec": ["./env_setup_petalinux_versal.sh"]
           }
        }

    }
}
